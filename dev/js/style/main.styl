

.web-tools-inner
  position: fixed
  width: 100%
  height: 100%
  top: 0
  left: 0
  z-index: 999


  .control
    position: absolute
    top: 0
    right: 0
    z-index: 2
    background-color: rgba(255, 255, 255, 0.8)
    padding: 10px
    width: 300px

    .space
      width: 100%
      height: 12px


  .imageboard
    position: absolute
    top: 0
    left: 0
    width: 100%
    height: 100%
    background-color: #000
    z-index: 1
    img
      cursor: move
